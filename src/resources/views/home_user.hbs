					<div class="search-input-container">
						<div class="search-input-inner">
							<div class="input-group">
								{{!-- <label for="name" class="form-label"></label> --}}
								<input class="form-control search-input-control" id="name" name="name" placeholder="Nhập tên bất động sản..." autocomplete="off">
							</div> 
						</div>
					</div>
				</div>
				<div class="col-1">
					<button class="btn-search" id="submit" type="submit" style="height: 43px;">
						<box-icon name='search-alt-2' animation='tada'></box-icon>
						<span>Tìm kiếm </span>
					</button>
				</div>
			</div>
			<div class="search-row g-2">
				<div class="col-10">
					<div class="row">
						<div class="col-3">
							<div class="dropdown-input">
								<div class="dropdown-section" id="City" data-control="search">
									<select for="add" id="add" name="add" class="form-control">
										<option selected >
											<span style="margin: auto;">Toàn quốc</span>
										</option>
										<option >Bến Tre</option>
										<option >Hồ Chí Minh</option>
										<option >Hà Nội</option>
									</select>
								</div>
							</div>
						</div>
						<div class="col-3">
							<div class="dropdown-input">
								<div class="dropdown-section" id="Price" data-control="search">
									<select for="price" id="price" name="price" class="form-control">
										<option selected >
											<span style="margin: auto;">Mức giá (/m2)</span>
										</option>
										<option >Dưới 100 000 000vnđ</option>
										<option >Dưới 300 000 000vnđ</option>
										<option >Dưới 1 000 000 000vnđ</option>
										<option >Trên 1 000 000 000vnđ</option>
									</select>
								</div>
							</div>
						</div>
						<div class="col-3">
							<div class="dropdown-input">
								<div class="dropdown-section" id="Area" data-control="search">
									<select for="area" id="area" name="area" class="form-control">
										<option selected value="all-area">
											<span style="margin: auto;">Diện tích (m2)</span>
										</option>
										<option >Dưới 100m2</option>
										<option >Dưới 300m2</option>
										<option >Trên 300m2</option>
									</select>
								</div>
							</div>
						</div>
						<div class="col-3">
							<div class="dropdown-input">
								<select class="form-control">
									<option selected>
										<span style="margin: auto;">Lọc thêm</span>
									</option>
								</select>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		</div>
	</form>
</section>
