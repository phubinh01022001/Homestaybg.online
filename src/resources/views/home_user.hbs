<section class="banner banner-home mb-4">
	<form class="search search-home" action="/homesUser/search_user" method="post">
		<div class="search-nav">
			<label class="search-nav-type">
				<input name="Type" aria-label="Search type" type="radio" checked="" value="1">
				<span class="nav-type-name">Tìm kiếm nhà đất</span>
			</label>
		</div>
		<div class="search-container">
			<div class="row g-2">
				<div class="col-1">

				</div>
